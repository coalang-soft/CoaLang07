def strChars [ str ] [
	str @ toCharArray ? 0
] ! ;
def strReverse [ str ] [
	str strChars reverse / strConcat
] ! ;
def strAt [ str ; index ] [
	str strChars ? index
] ! ;
def strFromTo [ str ; from ; to ] [
	str strChars ? ( from .. to ) / ,,
] ! ;
def strLength [ str ] [
	str strChars length
] ! ;